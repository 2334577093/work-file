  "main": "index.js",
  "scripts": {
    "webpack": "webpack --config webpack.config.js --progress --display-modules --colors --display-reason",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --open  --host 0.0.0.0",
    "pro": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "tsa": "webpack-dev-server --hot --inline --content-base ./dist/",
    "test": "webpack-dev-server --port 3000 --hot --inline",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "kk": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --devtool eval --progress --color --profile"
  },
